{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": "./src/",
    "paths": {
      "@src/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@hooks/*": [
        "./hooks/*"
      ],
      "@navigations/*": [
        "./navigations/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@services/*": [
        "./services/*"
      ],
      "@config/*": [
        "./assets/config/*"
      ],
      "@utils/*": [
        "./utils/*"
      ]
    }
  }
}